<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>CampGamerApp</display-name>
  <welcome-file-list>
    <welcome-file>main.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>adminEmail</param-name>
    <param-value>stevenromero.dev@gmail.com</param-value>
  </context-param>
  <session-config>
    <session-timeout>15</session-timeout>
  </session-config>
  <listener>
    <listener-class>listeners.SessionCounter</listener-class>
  </listener>
  <servlet>
    <servlet-name>Main</servlet-name>
    <servlet-class>main.jsp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>example.HelloServlet</servlet-class>
    <init-param>
      <param-name>adminEmail</param-name>
      <param-value>stevenromero.dev@gmail.com</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddGame</servlet-name>
    <servlet-class>servlets.AddGameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddGame</servlet-name>
    <url-pattern>/addGame</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CustomBlogServlet</servlet-name>
    <servlet-class>servlets.CustomBlogServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CustomBlogServlet</servlet-name>
    <url-pattern>/blog</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CustomNews</servlet-name>
    <servlet-class>servlets.CustomNewsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CustomNews</servlet-name>
    <url-pattern>/news</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetGame</servlet-name>
    <servlet-class>servlets.GetGameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetGame</servlet-name>
    <url-pattern>/getGame</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetVideo</servlet-name>
    <servlet-class>servlets.GetVideoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetVideo</servlet-name>
    <url-pattern>/getVideo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UploadVideo</servlet-name>
    <servlet-class>servlets.UploadVideoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadVideo</servlet-name>
    <url-pattern>/uploadVideo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetUserProfileData</servlet-name>
    <servlet-class>servlets.GetUserProfileDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetUserProfileData</servlet-name>
    <url-pattern>/getUserData</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogIn</servlet-name>
    <servlet-class>servlets.LogInServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogIn</servlet-name>
    <url-pattern>/logIn</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogOut</servlet-name>
    <servlet-class>servlets.LogOutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogOut</servlet-name>
    <url-pattern>/logOut</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterUser</servlet-name>
    <servlet-class>servlets.RegisterUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterUser</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UploadImages</servlet-name>
    <servlet-class>servlets.UploadImagesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadImages</servlet-name>
    <url-pattern>/uploadImage</url-pattern>
  </servlet-mapping>
</web-app>